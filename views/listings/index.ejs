<% layout("./layouts/boilerplate")%>

<%= success %>
<body>
    


<div class="row row-cols-lg-3 row-cols-md-2 row-cols-sm-1">

  <% for(data of allListings){%>
  <a href="/listings/<%= data._id%>" >
    <div class="card listing-card" >
      <img src="<%= data.image%>" class="card-img-top" alt="listing_img" height="20rem">
      <div class="card-img-overlay"></div>
      <div class="card-body">
        <p class="card-text"><b><%= data.title %></b> <br>
          &#8377 <%= data.price.toLocaleString("en-IN") %>  /night
       </p>
      </div>
    </div>
    
    <% }%>
</div>

</a>


  

  








</body>